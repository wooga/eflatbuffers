// Currently only supports one attribute per schema
// Does not support built in attributes

attribute "priority";
attribute "name";

enum Color (priority: 3) : byte { Red, Green, Blue }
union Animal (priority: 4) { Dog, Cat }

table Dog {
  age:int;
}

table Cat {
  lives:int;
}

